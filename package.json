{
  "name": "biohackstack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "landing:dev": "vite --config vite.config.landing.ts",
    "landing:build": "vite build --config vite.config.landing.ts --outDir dist-landing",
    "test": "vitest run",
  "test:e2e": "playwright test",
  "test:e2e:ci": "npx playwright install --with-deps && playwright test --reporter=list",
  "dev:ci": "vite --host 127.0.0.1 --port 5173 --strictPort",
  "e2e:local": "npx start-server-and-test \"npm run dev:ci\" http://127.0.0.1:5173 \"npx playwright test\"",
  "e2e:prod": "npx vite build && node scripts/e2e-prod-run.cjs",
    "test:integration:emulator": "node -e \"if (!process.env.FIRESTORE_EMULATOR_HOST) { console.error('Set FIRESTORE_EMULATOR_HOST to run emulator integration tests'); process.exit(1) } else { require('child_process').execSync('vitest run tests/dataStore.integration.test.ts', { stdio: 'inherit' }) }\"",
    "serve:dist": "http-server ./dist -p 3000 -c-1",
    "sitemap:serve": "node server.js"
  },
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "ethers": "^6.13.1",
    "firebase": "^12.0.0",
    "markdown-it": "^14.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.4.1",
    "vitest": "^3.2.4",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^22.14.0",
    "firebase-tools": "^14.16.0",
    "http-server": "^14.1.1",
    "jsdom": "^23.0.0",
    "start-server-and-test": "^2.1.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
